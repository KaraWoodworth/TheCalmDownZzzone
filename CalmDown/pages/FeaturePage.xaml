<Page x:Class="CalmDown.pages.FeaturePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CalmDown.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CoreTest"
      ShowsNavigationUI="False">

    <Grid>
        <Label x:Name="pageLabel" Content="Title" FontSize="64" HorizontalAlignment="Center"/>
        <Button x:Name="pageButton" Content="Button" Margin="252,0,268,10" Style="{StaticResource RoundButtonTemplate}" Click="GenerateLabel" FontFamily="Liberation Sans" Height="66" VerticalAlignment="Bottom"/>
        <Image x:Name="pageImage" Height="286" Margin="10,83,10,0" VerticalAlignment="Top"/>
        

    </Grid>
</Page>
